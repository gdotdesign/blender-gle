---JS
window.addEvent('domready', function(){
  group = new Core.PushGroup();
  for(i=0;i<5;i++){
    group.addItem(new Core.Push({label:i}));  
  }
  slider = new Data.Number({label:'Size'})
  slider.addEvent('change',function(value){
    group.set('size', value)
  });
  slider.set('value',200)
  $$(".demo").grab(group);
  $("options").adopt(slider);
});
